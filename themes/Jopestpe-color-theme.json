	"tokenColors": [
		{ 	// if, else, switch, etc.
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#FC8BCA"
			}
		},
		{ 	// Nome da função
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#57B2FF"
			}
		},
		{ 	// Strings
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#FFEDA1"
			}
		},
		{ 	// Comentarios
			"scope": "comment",
			"settings": {
				"foreground": "#CECECE"
			}
		},
		{ 	// Funcao, Atributos
			"scope": [
				"storage.type.function",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ff7084"
			}
		},
		{ 	// Tags em HTML, XML e similares, chaves em dicionários e objetos,argumentos nomeados e variáveis
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#63C259"
			}
		},
	]
}