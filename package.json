{
  "name": "jopestpe-tema",
  "displayName": "Jopestpe Tema",
  "description": "Tema de cores escuro layout",
  "publisher": "Jopestpe",
  "version": "0.0.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "assets/icone.png", 
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:AtivarLayoutJopestpe",
    "onCommand:DesativarLayoutJopestpe"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Jopestpe Tema",
        "uiTheme": "vs-dark",
        "path": "./themes/Jopestpe-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "AtivarLayoutJopestpe",
        "title": "Ativar Layout Jopestpe"
      },
      {
        "command": "DesativarLayoutJopestpe",
        "title": "Desativar Layout Jopestpe"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jopestpe/vscode-tema.git"
  }
}
